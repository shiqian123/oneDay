{
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": [0, "never"],
    "key-spacing": [0],
    "keyword-spacing": [0],
    "max-len":[0],
    "semi":[0],
    "no-use-before-define":[0],
    "space-before-blocks": [0],
    "indent":[0],
    "no-var":[0],
    "one-var":[0],
    "no-console":[0],
    "no-unused-vars":[0],
    "guard-for-in":[0],
    "no-restricted-syntax":[0],
    "comma-dangle":[0],
    "comma-spacing":[0],
    "arrow-body-style":[0],
    "import/imports-first":[0],
    "arrow-spacing":[0],
    "one-var-declaration-per-line":[0],
    "prefer-template":[0],
    "prefer-const":[0],
    "react/jsx-no-bind":[0],
    "eol-last":[0],
    "space-infix-ops":[0],
    "object-shorthand":[0],
    " react/sort-comp":[0],
    "quotes":[0],
    "react/jsx-equals-spacing":[0],
    "prefer-arrow-callback":[0],
    "react/jsx-indent-props":[0],
    "func-names":[0],
    "jsx-a11y/img-has-alt":[0],
    "react/prop-types":[0],
    "react/sort-comp":[0],
    "react/self-closing-comp":[0],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export":[0],
    "brace-style":[0],
    "block-spacing":[0],
    "react/jsx-indent":[0],
    "react/no-string-refs":[0],
    "react/jsx-first-prop-new-line":[0]
  },
  "env" :{
    "browser": true,
  }
}
